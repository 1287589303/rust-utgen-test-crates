  "code": [
    "fn put<T: super::Buf>(&mut self, mut src: T)",
    "where",
    "    Self: Sized,",
    "{",
    "    // In case the src isn't contiguous, reserve upfront.",
    "    self.reserve(src.remaining());",
    "",
    "    while src.has_remaining() {",
    "        let s = src.chunk();",
    "        let l = s.len();",
    "        self.extend_from_slice(s);",
    "        src.advance(l);",
    "    }",
    "}"
