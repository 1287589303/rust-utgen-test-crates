          "    self.wtr.write_str(r\"(?-u:\\B)\")? == Ok(())",
          "    assert_eq!(visitor.visit_pre(&hir), Ok(()))",
          "    assert!(writer.output.contains(r\"(?-u:\\B)\"))",
          "    writer.output.len() > 0"
