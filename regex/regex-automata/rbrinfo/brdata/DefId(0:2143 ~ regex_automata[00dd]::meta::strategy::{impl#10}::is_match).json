    "        }",
    "        Err(RetryError::Fail(_err)) => {",
    "            trace!(\"reverse inner fast half search failed: {}\", _err);",
    "            self.core.is_match_nofail(cache, input)",
    "        }",
    "        Ok(None) => false,",
    "        Ok(Some(_)) => true,",
    "    }",
    "}"
